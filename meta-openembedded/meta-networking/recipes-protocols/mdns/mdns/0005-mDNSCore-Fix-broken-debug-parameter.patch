                         {
                             CacheRecord *negcr;
                             const DNSServRef dnsserv = qptr->qDNSServer;
-                            debugf("mDNSCoreReceiveNoUnicastAnswers making negative cache entry TTL %d for %##s (%s)", negttl, currentQName, DNSTypeName(q.qtype));
